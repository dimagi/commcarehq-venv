#!/usr/bin/env python2.7

import os; activate_this=os.path.join(os.path.dirname(os.path.realpath(__file__)), 'activate_this.py'); execfile(activate_this, dict(__file__=activate_this)); del os, activate_this

# EASY-INSTALL-ENTRY-SCRIPT: 'celerymon==1.0.3','console_scripts','celerymon'
__requires__ = 'celerymon==1.0.3'
import sys
from pkg_resources import load_entry_point

sys.exit(
   load_entry_point('celerymon==1.0.3', 'console_scripts', 'celerymon')()
)
#!/home/travis/virtualenv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'coveralls==0.2','console_scripts','coveralls'
__requires__ = 'coveralls==0.2'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('coveralls==0.2', 'console_scripts', 'coveralls')()
    )

#!/home/travis/virtualenv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'sniffer==0.3.5','console_scripts','sniffer'
__requires__ = 'sniffer==0.3.5'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('sniffer==0.3.5', 'console_scripts', 'sniffer')()
    )

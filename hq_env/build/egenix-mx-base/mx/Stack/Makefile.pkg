all:	compile install

compile:
	cd mxStack; \
	if ! test -f Makefile; then $(MAKE) -f Makefile.pre.in boot; fi; \
	$(MAKE)

install:
	cd mxStack; \
	$(MAKE) install

test:
	cd mxStack; \
	python test.py

doc:
	-true


# Cleanup before distributing the package
#
distclean:
	cd mxStack; \
	$(MAKE) distclean
